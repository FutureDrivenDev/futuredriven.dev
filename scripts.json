{
  "$schema": "https://deno.land/x/denon/schema.json",
  "scripts": {
    "dev": "deno run -A --unstable --no-check src/server.ts"
  },
  "watcher": {
    "exts": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "md"
    ],
    "match": ["deno.json", "src/**/*.ts", "src/**/*.tsx", "public/*"]
  }
}
