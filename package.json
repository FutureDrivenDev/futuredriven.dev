{
  "name": "futuredriven.dev",
  "version": "0.0.1",
  "description": "Landing pages and blog for Future Driven.",
  "scripts": {
    "build": "windicss --output public/windi.css --minify src/components/*.tsx",
    "dev": "concurrently -n \"windi,deno\" -c \"green,blue\" \"npm run windi\" \"denon start\"",
    "windi": "windicss --output public/windi.css --dev src/components/*.tsx",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "serve": "npm run build && denon start",
    "start": "npm run dev",
    "test": "echo 'soon...'"
  },
  "devDependencies": {
    "autoprefixer": "10.4.2",
    "concurrently": "^7.0.0",
    "prettier": "2.3.0",
    "typescript": "^4.5.2",
    "windicss": "^3.4.3"
  },
  "author": "Guilherme Rodrigues",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.2"
  }
}
