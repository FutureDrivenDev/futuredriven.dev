{
  "name": "mesa.land",
  "version": "0.0.1",
  "description": "Blank starter app",
  "scripts": {
    "build": "npm run build.server && npm run build.client",
    "build.client": "vite build",
    "build.server": "vite build --outDir server/build --ssr src/entry.server.tsx",
    "dev": "vite",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "serve": "denon start",
    "start": "npm run dev",
    "postinstall": "deno run --allow-read --allow-write HACK__patch_vite.ts"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.16-10",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.6",
    "prettier": "2.3.0",
    "tailwindcss": "3.0.18",
    "typescript": "^4.5.2",
    "vite": "2.7.13"
  },
  "author": "Builder.io Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.14.0"
  }
}
