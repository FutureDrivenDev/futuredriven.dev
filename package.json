{
  "name": "mesa.land",
  "version": "0.0.1",
  "description": "Blank starter app",
  "scripts": {
    "build": "npm run build.client && npm run build.server",
    "build.client": "vite build",
    "build.server": "vite build --outDir server/build --ssr src/entry.cloudflare.tsx",
    "deploy": "wrangler publish",
    "deploy.dev": "wrangler dev",
    "deploy.preview": "wrangler preview",
    "dev": "vite",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.16-10",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "3.2.0",
    "@cloudflare/wrangler": "1.19.7",
    "@types/service-worker-mock": "2.0.1",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.6",
    "prettier": "2.3.0",
    "tailwindcss": "3.0.18",
    "typescript": "^4.5.2",
    "vite": "2.7.13"
  },
  "author": "Builder.io Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.14.0"
  },
  "dependencies": {
    "postcss-d-ts": "^1.2.0"
  }
}
